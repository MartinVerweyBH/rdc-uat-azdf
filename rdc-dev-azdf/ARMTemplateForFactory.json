{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rdc-dev-azdf"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"azdl_ebs_gl_codecombinations_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azdl_ebs_gl_codecombinations'"
		},
		"lsEBS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsEBS'"
		},
		"lsEBS_PROD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsEBS_PROD'"
		},
		"lsEBS_UAT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsEBS_UAT'"
		},
		"lsMATRIX_Postgres_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMATRIX_Postgres'"
		},
		"lsMRTG_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMRTG'"
		},
		"lsMRTG_Postgres_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMRTG_Postgres'"
		},
		"lsMTRXUATC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMTRXUATC'"
		},
		"lsMTRXUATP_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMTRXUATP'"
		},
		"lsOAC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lsOAC'"
		},
		"lsOACPROD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsOACPROD'"
		},
		"lsSFDC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lsSFDC'"
		},
		"lsSFDC_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'lsSFDC'"
		},
		"ls_dbStageRDC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dbStageRDC'"
		},
		"ls_dwStageOAC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dwStageOAC'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://blobdevstorgen01.dfs.core.windows.net"
		},
		"AzureDataLakeStoreGen1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://devls.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStoreGen1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "be8c0789-cb3d-4f3a-94d7-7c9301ca4803"
		},
		"AzureDataLakeStoreGen1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "a0c1c7fd-38b0-422f-8a34-8d9d015a9b00"
		},
		"AzureDataLakeStoreGen1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "rdc-dev-rg"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://graph.microsoft.com/"
		},
		"RestService1_properties_typeProperties_aadResourceId": {
			"type": "string",
			"defaultValue": "https://management.core.windows.net"
		},
		"azdl_ebs_gl_codecombinations_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://blobdevstorgen01.dfs.core.windows.net"
		},
		"lsOAC_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "129.150.93.89"
		},
		"lsOAC_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "oacprod"
		},
		"lsSFDC_properties_typeProperties_environmentUrl": {
			"type": "string",
			"defaultValue": "https://ragingwire.my.salesforce.com"
		},
		"lsSFDC_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "svc_powerbisfdc@ragingwire.com"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStoreGen1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "[parameters('AzureDataLakeStoreGen1_properties_typeProperties_dataLakeStoreUri')]",
					"tenant": "[parameters('AzureDataLakeStoreGen1_properties_typeProperties_tenant')]",
					"subscriptionId": "[parameters('AzureDataLakeStoreGen1_properties_typeProperties_subscriptionId')]",
					"resourceGroupName": "[parameters('AzureDataLakeStoreGen1_properties_typeProperties_resourceGroupName')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/RestService1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "RestService",
				"typeProperties": {
					"url": "[parameters('RestService1_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "ManagedServiceIdentity",
					"aadResourceId": "[parameters('RestService1_properties_typeProperties_aadResourceId')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/azdl_ebs_gl_codecombinations')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('azdl_ebs_gl_codecombinations_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('azdl_ebs_gl_codecombinations_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsEBS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "link service to Oracle EBS on Site",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsEBS_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsEBS_PROD')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service to EBS On Prem Production (OFINPROD)",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsEBS_PROD_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsEBS_UAT')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "link service to Oracle EBS on Site UAT",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsEBS_UAT_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsMATRIX_Postgres')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "lsMATRIX_Postgres",
				"annotations": [],
				"type": "PostgreSql",
				"typeProperties": {
					"connectionString": "[parameters('lsMATRIX_Postgres_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsMRTG')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service to MRTG On Prem",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsMRTG_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsMRTG_Postgres')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service to MRTG on Postgres (On Prem)",
				"annotations": [],
				"type": "PostgreSql",
				"typeProperties": {
					"connectionString": "[parameters('lsMRTG_Postgres_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsMTRXUATC')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service to MTRXUATC",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsMTRXUATC_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsMTRXUATP')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service to Matrix on-prem MTRXUATP",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsMTRXUATP_connectionString')]"
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsOAC')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service to Oracle OAC",
				"annotations": [],
				"type": "OracleServiceCloud",
				"typeProperties": {
					"host": "[parameters('lsOAC_properties_typeProperties_host')]",
					"username": "[parameters('lsOAC_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('lsOAC_password')]"
					},
					"useEncryptedEndpoints": true,
					"useHostVerification": true,
					"usePeerVerification": true
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsOACPROD')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service OAC PROD",
				"annotations": [],
				"type": "Oracle",
				"typeProperties": {
					"connectionString": "[parameters('lsOACPROD_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsSFDC')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link Service Sales Force Dot Com",
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('lsSFDC_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('lsSFDC_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('lsSFDC_password')]"
					},
					"securityToken": {
						"type": "SecureString",
						"value": "[parameters('lsSFDC_securityToken')]"
					}
				},
				"connectVia": {
					"referenceName": "rdcir01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/rdcir01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_dbStageRDC')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('ls_dbStageRDC_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_dwStageOAC')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "link service Azure SQL - dwStageOAC   ",
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('ls_dwStageOAC_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Oracle')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/azureintegrationRuntime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/rdcir01')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}