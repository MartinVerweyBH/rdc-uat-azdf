{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rdc-dev-azdf"
		},
		"lsEBS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsEBS'"
		},
		"lsEBS_PROD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsEBS_PROD'"
		},
		"lsEBS_UAT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsEBS_UAT'"
		},
		"lsMATRIX_Postgres_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMATRIX_Postgres'"
		},
		"lsMRTG_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMRTG'"
		},
		"lsMRTG_Postgres_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsMRTG_Postgres'"
		},
		"lsOACPROD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsOACPROD'"
		},
		"lsSFDC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lsSFDC'"
		},
		"lsSFDC_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'lsSFDC'"
		},
		"ls_dwStageOAC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dwStageOAC'"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://graph.microsoft.com/"
		},
		"RestService1_properties_typeProperties_aadResourceId": {
			"type": "string",
			"defaultValue": "https://management.core.windows.net"
		},
		"lsSFDC_properties_typeProperties_environmentUrl": {
			"type": "string",
			"defaultValue": "https://ragingwire.my.salesforce.com"
		},
		"lsSFDC_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "svc_powerbisfdc@ragingwire.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsOACPROD_connectionString": {
						"value": "[parameters('lsOACPROD_connectionString')]"
					},
					"ls_dwStageOAC_connectionString": {
						"value": "[parameters('ls_dwStageOAC_connectionString')]"
					},
					"lsEBS_connectionString": {
						"value": "[parameters('lsEBS_connectionString')]"
					},
					"lsEBS_PROD_connectionString": {
						"value": "[parameters('lsEBS_PROD_connectionString')]"
					},
					"lsEBS_UAT_connectionString": {
						"value": "[parameters('lsEBS_UAT_connectionString')]"
					},
					"lsMATRIX_Postgres_connectionString": {
						"value": "[parameters('lsMATRIX_Postgres_connectionString')]"
					},
					"lsMRTG_connectionString": {
						"value": "[parameters('lsMRTG_connectionString')]"
					},
					"lsMRTG_Postgres_connectionString": {
						"value": "[parameters('lsMRTG_Postgres_connectionString')]"
					},
					"lsSFDC_password": {
						"value": "[parameters('lsSFDC_password')]"
					},
					"lsSFDC_securityToken": {
						"value": "[parameters('lsSFDC_securityToken')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_aadResourceId": {
						"value": "[parameters('RestService1_properties_typeProperties_aadResourceId')]"
					},
					"lsSFDC_properties_typeProperties_environmentUrl": {
						"value": "[parameters('lsSFDC_properties_typeProperties_environmentUrl')]"
					},
					"lsSFDC_properties_typeProperties_username": {
						"value": "[parameters('lsSFDC_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}